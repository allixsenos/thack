{
  "manifest_version": 2,
  "name": "Thack App",
  "version": "0.1",

  "description": "This is a pretty cool app, believe me",

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "permissions": [
    "geolocation",
    "https://spreadsheets.google.com/"
  ],

  "content_security_policy": "script-src 'self' https://spreadsheets.google.com; object-src 'self'"
}
